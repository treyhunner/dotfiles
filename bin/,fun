""".splitlines()


match sys.argv[1:]:
    case []:
        nums = [1]
    case ["--all"]:
        nums = [len(chars)]
    case [*nums]:
        nums = [
            min(int(n), len(chars))
            for n in nums
        ]

for n in nums:
    print(*random.sample(chars, k=n))
